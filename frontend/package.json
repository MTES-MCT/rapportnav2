{
  "name": "frontend",
  "version": "1.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:with-sourcemaps": "vite build --sourcemap",
    "tsc": "tsc",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 10000",
    "preview": "vite preview",
    "test": "vitest run src",
    "test:watch": "vitest src",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@apollo/client": "^3.9.4",
    "@mtes-mct/monitor-ui": "^11.8.0",
    "@sentry/integrations": "^7.93.0",
    "@sentry/react": "^7.93.0",
    "apollo3-cache-persist": "^0.14.1",
    "date-fns": "^2.30.0",
    "email-validator": "^2.0.4",
    "formik": "^2.4.5",
    "graphql": "^16.8.1",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.0",
    "react-toastify": "^9.1.3",
    "rsuite": "^5.44.0",
    "styled-components": "^5.3.11",
    "type-fest": "^4.8.2",
    "uuid": "^9.0.1",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^27.5.2",
    "@types/node": "^20.10.1",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@types/styled-components": "^5.1.26",
    "@types/uuid": "^9.0.4",
    "@vitejs/plugin-react": "^4.0.4",
    "@vitest/coverage-istanbul": "^1.1.3",
    "eslint": "^8.56.0",
    "eslint-config-react-app": "^7.0.1",
    "jest-watch-typeahead": "^2.2.2",
    "jsdom": "^22.1.0",
    "msw": "^2.0.11",
    "typescript": "^5.3.2",
    "vite": "^4.5.2",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-svgr": "^3.2.0",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^1.1.3"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "^4.9.6"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:80",
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!(@mtes-mct/monitor-ui))/"
    ]
  }
}
