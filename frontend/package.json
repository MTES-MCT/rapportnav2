{
  "name": "frontend",
  "version": "2.3.5",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "env-prepare": "import-meta-env-prepare -x ./.env.local.defaults",
    "dev": "npm run env-prepare && vite",
    "build": "npm run env-prepare && vite build && import-meta-env -x .env.example -p dist/index.html",
    "build:with-sourcemaps": "vite build --sourcemap",
    "tsc": "tsc",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 10000",
    "preview": "vite preview",
    "test": "vitest run src",
    "test:watch": "vitest src",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@apollo/client": "^3.11.10",
    "@mtes-mct/monitor-ui": "^24.10.2",
    "@sentry/react": "^8.41.0",
    "@unleash/proxy-client-react": "^4.3.1",
    "apollo3-cache-persist": "^0.15.0",
    "date-fns": "3.6.0",
    "date-fns-tz": "^3.2.0",
    "dotenv": "^16.4.5",
    "email-validator": "^2.0.4",
    "formik": "^2.4.6",
    "graphql": "^16.9.0",
    "jwt-decode": "^4.0.0",
    "lodash": "^4.17.21",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.28.0",
    "react-toastify": "^9.1.3",
    "rsuite": "5.53.2",
    "styled-components": "5.3.10",
    "type-fest": "^4.29.0",
    "unleash-proxy-client": "^3.5.1",
    "uuid": "^9.0.1",
    "web-vitals": "^2.1.4",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@import-meta-env/cli": "^0.6.8",
    "@import-meta-env/prepare": "0.1.13",
    "@import-meta-env/unplugin": "^0.5.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^27.5.2",
    "@types/node": "^20.10.1",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@types/styled-components": "^5.1.26",
    "@types/uuid": "^9.0.4",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-istanbul": "^1.1.3",
    "eslint": "^8.57.0",
    "eslint-config-react-app": "^7.0.1",
    "jest-watch-typeahead": "^2.2.2",
    "jsdom": "^22.1.0",
    "msw": "^2.4.7",
    "prettier": "3.2.5",
    "typescript": "^5.4.3",
    "vite": "^5.4.6",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-svgr": "github:pd4d10/vite-plugin-svgr#main",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^1.4.0"
  },
  "overrides": {
    "rollup": "4.22.4"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "^4.9.6"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:80",
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!(@mtes-mct/monitor-ui))/"
    ]
  },
  "lint-staged": {
    "*.{json,md,yaml,yml}": "prettier --write",
    "*.{ts,tsx}": [
      "eslint --fix",
      "bash -c 'npm run lint -- --fix'"
    ]
  }
}
