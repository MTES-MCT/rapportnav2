type Query {
  missions(userId: ID): [Mission]!
  mission(missionId: ID): Mission!

}

type Mutation {
  updateMission(mission: MissionInput!): Mission!
}

input MissionInput {
  id: ID!
  startDateTimeUtc: String!
  endDateTimeUtc: String!
}

type Mission {
  id: ID!
  isClosed: Boolean!
  missionSource: String!
  startDateTimeUtc: String!
  endDateTimeUtc: String!
  actions: [Action]
}
