extend type Query {
  agents: [Agent]
  agentsByServiceId(serviceId: ID): [Agent]
  agentsByMissionId(missionId: ID): [Agent]
  agentRoles: [AgentRole]
}
type Agent {
    id: ID!
    firstName: String!
    lastName: String!
    services: [Service]
}

type Service {
  id: ID!
  name: String!
}

type AgentRole {
  id: ID!
  title: String!
}
