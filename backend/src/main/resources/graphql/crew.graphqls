extend type Query {
  agents: [Agent]
  agentsByServiceId(serviceId: ID): [Agent]
  agentCrewByMissionId(missionId: ID): [AgentCrew]
  agentRoles: [AgentRole]
}
type Agent {
    id: ID!
    firstName: String!
    lastName: String!
    services: [Service]
}

type Service {
  id: ID!
  name: String!
}

type AgentRole {
  id: ID!
  title: String!
}

type AgentCrew {
  id: ID!
  agent: Agent
  comment: String
  role: AgentRole
}
